{
  "name": "@bemedev/decompose",
  "version": "1.4.0",
  "description": "Decompose object and so more",
  "author": {
    "email": "bri_lvi@icloud.com",
    "name": "chlbri",
    "url": "https://bemedev.vercel.app"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chlbri/decompose"
  },
  "license": "MIT",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "module": "lib/index.js",
  "keywords": [
    "For",
    "creating",
    "new",
    "npm",
    "package"
  ],
  "engines": {
    "node": ">=20"
  },
  "files": [
    "lib",
    "public"
  ],
  "scripts": {
    "config": "pnpm install",
    "config:off": "pnpm run config --offline",
    "ci": "pnpm run config:off && pnpm run lint && pnpm run test",
    "ci:admin": "pnpm run rm && pnpm run upgrade && pnpm run ci",
    "rollup": "rollup -c",
    "build": "rm -rf lib && pnpm run rollup",
    "pretest": "pnpm run build && pnpm build-tests pre",
    "posttest": "pnpm build-tests post && pnpm run prettier && pnpm run p-q",
    "rm": "pnpx rimraf -rf node_modules pnpm-lock.yaml",
    "p-q": "pretty-quick",
    "rinit": "pnpm run rm && pnpm run config",
    "rinit:off": "pnpm run rm && pnpm run config:off",
    "test": "pnpm run test:coverage --no-coverage",
    "test:coverage": "vitest run --passWithNoTests",
    "test:watch": "vitest --passWithNoTests",
    "remove": "rm -rf lib node_modules pnpm-lock.yaml",
    "clean": "pnpm run remove && pnpm run config",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "eslint": "eslint \"{src/**/*,App}.{ts,tsx}\"",
    "lint": "pnpm run prettier && pnpm run eslint --fix",
    "lint-watch": "onchange --await-write-finish 5500 \"**/*\" -- pnpm run lint",
    "prepublishOnly": "",
    "prepare": "husky",
    "upgrade": "pnpm up --latest && pnpm add -D vitest@3.2.4 @vitest/coverage-v8@3.2.4"
  },
  "devDependencies": {
    "@bemedev/fsf": "^1.0.1",
    "@bemedev/rollup-config": "^0.2.2",
    "@bemedev/vitest-alias": "^0.0.3",
    "@bemedev/vitest-exclude": "^0.1.1",
    "@bemedev/vitest-extended": "^1.5.3",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.2",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.39.2",
    "glob": "^13.0.0",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "immer": "^11.0.1",
    "onchange": "^7.1.0",
    "prettier": "^3.7.4",
    "pretty-quick": "^4.2.2",
    "rollup": "^4.53.4",
    "rollup-plugin-tsc-alias": "^1.1.4",
    "rollup-plugin-typescript2": "^0.36.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@bemedev/build-tests": "^0.1.2",
    "ts-deepmerge": "^7.0.3"
  },
  "peerDependencies": {
    "@bemedev/types": "0.4.2"
  }
}
